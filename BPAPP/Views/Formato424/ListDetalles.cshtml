@model IEnumerable<ProyectoWeb.Models.Form424ConsultaDetalle>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Subcuenta)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idOperacionServicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idCanal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumOperServiciosCuotamanejo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CostoFijo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CostoProporcionOperacionServicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Observaciones)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Subcuenta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OperacionServicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Canal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumOperServiciosCuotamanejo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CostoFijo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CostoProporcionOperacionServicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Observaciones)
        </td>
        <td>
            @Html.ActionLink("Editar", "UpdateDetalle", new {  id=item.idDetalle })
        </td>
    </tr>
}

</table>